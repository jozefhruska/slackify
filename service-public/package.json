{
  "name": "slackify-service-public",
  "version": "0.1.0",
  "scripts": {
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly src/index.ts",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "nexus:generate": "ts-node --transpile-only src/schema",
    "nexus:generate:watch": "ts-node-dev --no-notify --transpileOnly --respawn src/schema"
  },
  "author": "Jozef Hru≈°ka (xhrusk25@vutbr.cz)",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "~2.0.0-preview022",
    "apollo-server": "~2.10.1",
    "axios": "~0.19.2",
    "dotenv": "~8.2.0",
    "graphql": "~14.6.0",
    "nexus": "~0.12.0-rc.13",
    "nexus-prisma": "~0.10.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "~2.20.0",
    "@typescript-eslint/parser": "~2.20.0",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "~6.10.0",
    "eslint-plugin-prettier": "~3.1.2",
    "prettier": "~1.19.1",
    "ts-node": "~8.6.2",
    "ts-node-dev": "~1.0.0-pre.44",
    "typescript": "~3.8.2"
  }
}
