    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "graphql": "^14.6.0",
    "http-errors": "^1.7.3",
    "nexus": "^0.12.0-rc.11",
    "nexus-prisma": "^0.9.1",
    "qs": "^6.9.1",
    "raw-body": "^2.4.1",
    "tsscmp": "^1.0.6"
  },
