    "jsonwebtoken": "~8.5.1",
    "jwt-decode": "~2.2.0",
    "nexus": "~0.12.0",
    "nexus-prisma": "~0.12.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "~8.3.9",
    "@types/jwt-decode": "~2.2.1",
    "@typescript-eslint/eslint-plugin": "~2.31.0",
    "@typescript-eslint/parser": "~2.31.0",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-prettier": "~3.1.2",
    "prettier": "~2.0.4",
